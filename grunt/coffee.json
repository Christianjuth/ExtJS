{
  "default" : {
    "files" : [
      {
        "expand" : true,
        "cwd" : "app/coffee",
        "src" : ["**/*.coffee"],
        "dest" : "builds/latest.safariextension/assets/js/<%= srcDir %>/",
        "ext" : ".js"
      },
      {
        "expand" : true,
        "cwd" : "app/views",
        "src" : ["**/*.coffee"],
        "dest" : "builds/latest.safariextension/views/<%= srcDir %>/",
        "ext" : ".js"
      }
    ]
  }
}
