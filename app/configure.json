{
  "author" : "Christian Juth",
  "name":"extension_framework",
  "version" : "0.1.1",

  "storage" : [
    {
      "key" : "google",
      "default":"0"
    },
    {
      "key" : "microsoft",
      "default":"0"
    },
    {
      "key" : "xbox",
      "default":"0"
    }
  ],

  "options" : [
    {
      "title" : "Name",
      "type" : "text",
      "key" : "name",
      "default":"John"
    },

    {
      "title" : "Age",
      "type" : "number",
      "key":"age",
      "default":"18"
    },

    {
      "title" : "Setting",
      "type" : "text",
      "key":"setting",
      "default":"hey"
    },

    {
      "title" : "Favorite Color",
      "type" : "text",
      "key":"color",
      "default":"blue"
    },

    {
      "title" : "Favorite Food",
      "type" : "text",
      "key" : "food",
      "default":"Pizza"
    }

  ]
}
